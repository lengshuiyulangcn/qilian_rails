<table class="table">
    <thead>
      <tr>
        <th>姓名</th>
        <th>性别</th>
        <th>Email</th>
        <th>联系电话</th>
	<th>微信</th>
      </tr>
    </thead>
    <tbody>
     <% users.each do  |user|%>
      <tr>
	<td><%= user.family_name+" "+ user.given_name %></td>
	<td><%= user.name%></td>
	<td><%= user.email%></td>
	<td><%= user.phone%></td>
	<td><%= user.wechat%></td>
      </tr>
    <% end %>
    </tbody>
</table>
  
